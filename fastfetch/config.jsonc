      "break",

      {
            "type": "custom",
            "key": "└─────────────────────────────────┘",
            "value": ""
          },
      {
      	"paddingLeft": 10,
        "type": "colors",
        "symbol": "circle"
      },
  
      "break",
      "break"
    ]
