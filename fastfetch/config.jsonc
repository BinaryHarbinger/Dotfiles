{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
      "type": "small",
      "padding": {
        "top": 4,
        "right": 4,
        "left": 4,
      }
    },
    "display": {
      "separator": " "
    },
    "modules": [
        "break",
        "break",
        {
        "type": "title",
        "format": "‎‎‎‎  ‎‎‎‎  ‎‎‎‎  {#1;34}{}{#}@{#1;34}{}{#}"
      },
      {
            "type": "custom",
            "key": "┌──────────────────────────────────┐",
            "value": ""
          },
      "break",
      {
        "type": "os",
        "format": "‎‎‎‎{3} {12}",
